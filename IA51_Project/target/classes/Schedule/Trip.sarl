package Schedule

import java.util.Date
import Road.*

/** 
 * @author 15800
 * 
 */
class Trip extends Object {
	public var id : int
	public var origin_name : Location
	public var start_time : int
	public var duration : int
	public var destination : Location
	public var mode : Transport_Mode
	public var path : Path
	/* constructor */

	new(id : int) {
		this.id = id
		this.origin_name = null
		this.start_time = -1
		this.duration = -1
		this.destination = null
		this.mode = null
	}
	
	new(id : int, ori:Location, st:int, d:int, des:Location, mode: Transport_Mode){
		this.id = id
		this.origin_name = ori
		this.start_time = st
		this.duration = d
		this.destination = des
		this.mode = mode
	}
	
}


